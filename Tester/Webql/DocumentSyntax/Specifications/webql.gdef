/*
	Document Syntax for WebQL Version 3.0.0, created by Rodrigo Jacques.
*/

start
	: json 
	;

document
	: object
	;

object
	: '{' { members } '}' 
	;

members
	: pair  { ',' members } 
	;

pair
	: $string ':' value 
	;

array
	: '[' [ elements ] ']'
	;

elements
	: value { ',' value }
	;

value 
	: number 
	| object 
	| array
	| $string
	| bool
	| null 
	;

bool
	: 'true' 
	| 'false' 
	;

null
	: 'null' 
	;

number
	: $int 
	| $float 
	| $hex 
	;
